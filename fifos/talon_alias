# CCD_VIS commands
camsky=/usr/local/telescope/comm/Camera.in "Expose 0+0x2048x2048 1x1 $1 1 0 $2\nOBJECT\nCOMMENT\nCOMMENT\nOBSERVER" /usr/local/telescope/comm/Camera.out $1+5 error unknown created "timed out"
camsubsky=/usr/local/telescope/comm/Camera.in "Expose $1+$2x$3x$4 1x1 $5 1 0 $6\nSOURCE\nCOMMENT\nTITLE\nOBSERVER" /usr/local/telescope/comm/Camera.out $1+5 error unknown created "timed out"
cambias=/usr/local/telescope/comm/Camera.in "Expose 0+0x2048x2048 1x1 0 0 0 $1\nSOURCE\nCOMMENT\nTITLE\nOBSERVER" /usr/local/telescope/comm/Camera.out 5 error unknown created "timed out"
camdark=/usr/local/telescope/comm/Camera.in "Expose 0+0x2048x2048 1x1 $1 0 0 $2\nSOURCE\nCOMMENT\nTITLE\nOBSERVER" /usr/local/telescope/comm/Camera.out $1+5 error unknown created "timed out"

# MEIA commands
meiasky=/usr/local/telescope/comm/Camera.in "Expose 0+0x2048x2048 1x1 $1 1 0 $2\nSOURCE\nCOMMENT\nTITLE\nOBSERVER" /usr/local/telescope/comm/Camera.out $1+5 error unknown created "timed out"
meiasubsky=/usr/local/telescope/comm/Camera.in "Expose $1+$2x$3x$4 1x1 $5 1 0 $6\nSOURCE\nCOMMENT\nTITLE\nOBSERVER" /usr/local/telescope/comm/Camera.out $1+5 error unknown created "timed out"
meiabias=/usr/local/telescope/comm/Camera.in "Expose 0+0x2048x2048 1x1 0 0 0 $1\nSOURCE\nCOMMENT\nTITLE\nOBSERVER" /usr/local/telescope/comm/Camera.out 5 error unknown created "timed out"
meiadark=/usr/local/telescope/comm/Camera.in "Expose 0+0x2048x2048 1x1 $1 0 0 $2\nSOURCE\nCOMMENT\nTITLE\nOBSERVER" /usr/local/telescope/comm/Camera.out $1+5 error unknown created "timed out"

# Camera commands (mainly for testing)
camerasky=/usr/local/telescope/comm/Camera.in "Expose 0+0x512x768 1x1 $1 1 0 $2\nSOURCE\nCOMMENT\nTITLE\nOBSERVER" /usr/local/telescope/comm/Camera.out $1+5 error unknown created "timed out"
camerabias=/usr/local/telescope/comm/Camera.in "Expose 0+0x512x768 1x1 0 0 0 $1\nSOURCE\nCOMMENT\nTITLE\nOBSERVER" /usr/local/telescope/comm/Camera.out 5 error unknown created "timed out"
cameradark=/usr/local/telescope/comm/Camera.in "Expose 0+0x512x768 1x1 $1 0 0 $2\nSOURCE\nCOMMENT\nTITLE\nOBSERVER" /usr/local/telescope/comm/Camera.out $1+5 error unknown created "timed out"
cameratest=/usr/local/telescope/comm/Camera.in "Expose 0+0x512x512 1x1 1 1 0 $1\nSOURCE\nCOMMENT\nTITLE\nOBSERVER" /usr/local/telescope/comm/Camera.out 60 error unknown created "timed out"
cameranewton=/usr/local/telescope/comm/Camera.in "Expose 0+0x2048x512 1x1 1 1 0 $1\nSOURCE\nCOMMENT\nTITLE\nOBSERVER" /usr/local/telescope/comm/Camera.out 60 error unknown created "timed out"
cameraikonl=/usr/local/telescope/comm/Camera.in "Expose 0+0x2088x2048 1x1 1 1 0 $1\nSOURCE\nCOMMENT\nTITLE\nOBSERVER" /usr/local/telescope/comm/Camera.out 60 error unknown created "timed out"
cameraexpose=/usr/local/telescope/comm/Camera.in "Expose $1+$2x$3x$4 $5x$6 $7 $8 $9 $10\n$11\n$12\n$13\n$14" /usr/local/telescope/comm/Camera.out 60 error unknown created "timed out"
camerastop=/usr/local/telescope/comm/Camera.in Stop /usr/local/telescope/comm/Camera.out 60 "stop complete"
camerareset=/usr/local/telescope/comm/Camera.in Reset /usr/local/telescope/comm/Camera.out 60 "reset complete"
cameratemperature=/usr/local/telescope/comm/Camera.in Temperature /usr/local/telescope/comm/Camera.out 1 "current temperature"

# Telescope commands
telhome=/usr/local/telescope/comm/Tel.in homeHD /usr/local/telescope/comm/Tel.out 90 "scope homing complete"
telhomeh=/usr/local/telescope/comm/Tel.in homeH /usr/local/telescope/comm/Tel.out 75 "scope homing complete"
telhomed=/usr/local/telescope/comm/Tel.in homeD /usr/local/telescope/comm/Tel.out 75 "scope homing complete"
telstow=/usr/local/telescope/comm/Tel.in Stow /usr/local/telescope/comm/Tel.out 60 "slew complete"
telstop=/usr/local/telescope/comm/Tel.in Stop /usr/local/telescope/comm/Tel.out 3 "stop complete"
telstatus=/usr/local/telescope/comm/Tel.in status /usr/local/telescope/comm/Tel.out 3 "altazimutal" "unknown"
#tellimits=/usr/local/telescope/comm/Tel.in Limits /usr/local/telescope/comm/Tel.out

# Telescope movements
telmove=/usr/local/telescope/comm/Tel.in "j$1" /usr/local/telescope/comm/Tel.out 10 "paddle command"
telmovenorth=/usr/local/telescope/comm/Tel.in "jN" /usr/local/telescope/comm/Tel.out 10 "paddle command up"
telmovesouth=/usr/local/telescope/comm/Tel.in "jS" /usr/local/telescope/comm/Tel.out 10 "paddle command down"
telmovewest=/usr/local/telescope/comm/Tel.in "jW" /usr/local/telescope/comm/Tel.out 10 "paddle command cw"
telmoveeast=/usr/local/telescope/comm/Tel.in "jE" /usr/local/telescope/comm/Tel.out 10 "paddle command ccw"
telmovestop=/usr/local/telescope/comm/Tel.in "j0" /usr/local/telescope/comm/Tel.out 10 "paddle command stop"
telstepnorth=/usr/local/telescope/comm/Tel.in "jN\nsleep 1\nj0" /usr/local/telescope/comm/Tel.out 10 "paddle command stop"
telstepsouth=/usr/local/telescope/comm/Tel.in "jS\nsleep 1\nj0" /usr/local/telescope/comm/Tel.out 10 "paddle command stop"
telstepwest=/usr/local/telescope/comm/Tel.in "jW\nsleep 1\nj0" /usr/local/telescope/comm/Tel.out 10 "paddle command stop"
telstepeast=/usr/local/telescope/comm/Tel.in "jE\nsleep 1\nj0" /usr/local/telescope/comm/Tel.out 10 "paddle command stop"

# Telescope tracking
teltrackepoch=/usr/local/telescope/comm/Tel.in "RA:$1 Dec:$2 Epoch:$3" /usr/local/telescope/comm/Tel.out 30 "now tracking" "error"
teltrackeod=/usr/local/telescope/comm/Tel.in "RA:$1 Dec:$2" /usr/local/telescope/comm/Tel.out 30 "now tracking" "error"

# Telescope slewing
telslewaltaz=/usr/local/telescope/comm/Tel.in "Alt:$1 Az:$2" /usr/local/telescope/comm/Tel.out 30 "slew complete"
telslewhadec=/usr/local/telescope/comm/Tel.in "HA:$1 Dec:$2" /usr/local/telescope/comm/Tel.out 30 "slew complete"

# Telescope relative movement
telxdelta=/usr/local/telescope/comm/Tel.in "xdelta($1,$2)" /usr/local/telescope/comm/Tel.out 30 "encoder steps" "now tracking" "lost tracking"

# Focus commands
focushome=/usr/local/telescope/comm/Focus.in home /usr/local/telescope/comm/Focus.out 60 "focus offset complete"
focuscheck=/usr/local/telescope/comm/Focus.in " " /usr/local/telescope/comm/Focus.out 5 "autofocus offset complete" "focus offset complete"
focusauto=/usr/local/telescope/comm/Focus.in auto /usr/local/telescope/comm/Focus.out 10 "autofocus offset complete" "Auto-focus enabled" 
focusstepin=/usr/local/telescope/comm/Focus.in "j+\nsleep 1\nj0" /usr/local/telescope/comm/Focus.out 6 "stop complete"
focusstepout=/usr/local/telescope/comm/Focus.in "j-\nsleep 1\nj0" /usr/local/telescope/comm/Focus.out 6 "stop complete"
focusin=/usr/local/telescope/comm/Focus.in j+ /usr/local/telescope/comm/Focus.out 60 "paddle command in"
focusout=/usr/local/telescope/comm/Focus.in j- /usr/local/telescope/comm/Focus.out 60 "paddle command out"
focusstop=/usr/local/telescope/comm/Focus.in j0 /usr/local/telescope/comm/Focus.out 3 "stop complete"
focusoffset=/usr/local/telescope/comm/Focus.in $1 /usr/local/telescope/comm/Focus.out 60 "focus offset complete"
focusstatus=/usr/local/telescope/comm/Focus.in status /usr/local/telescope/comm/Focus.out 3 

# Filter commands
filterhome=/usr/local/telescope/comm/Filter.in home /usr/local/telescope/comm/Filter.out 75 "filter in place"
filterright=/usr/local/telescope/comm/Filter.in j+ /usr/local/telescope/comm/Filter.out 60 "filter in place"
filterleft=/usr/local/telescope/comm/Filter.in j- /usr/local/telescope/comm/Filter.out 60 "filter in place"
filterset=/usr/local/telescope/comm/Filter.in $1 /usr/local/telescope/comm/Filter.out 60 "filter in place"
filterstatus=/usr/local/telescope/comm/Filter.in status /usr/local/telescope/comm/Filter.out 3 

# Dome commands
domehome=/usr/local/telescope/comm/Dome.in home /usr/local/telescope/comm/Dome.out 150 "home complete"
domeopen=/usr/local/telescope/comm/Dome.in open /usr/local/telescope/comm/Dome.out 150 "open complete"
domeclose=/usr/local/telescope/comm/Dome.in close /usr/local/telescope/comm/Dome.out 150 "close complete"
domeslew=/usr/local/telescope/comm/Dome.in Az:$1 /usr/local/telescope/comm/Dome.out 150 "azimuth command complete" "error"
domestepleft=/usr/local/telescope/comm/Dome.in "j+\nsleep 3\nj0" /usr/local/telescope/comm/Dome.out 6 "paddle command stop"
domestepright=/usr/local/telescope/comm/Dome.in "j-\nsleep 3\nj0" /usr/local/telescope/comm/Dome.out 6 "paddle command stop"
domeright=/usr/local/telescope/comm/Dome.in j+ /usr/local/telescope/comm/Dome.out 150 "paddle command cw"
domeleft=/usr/local/telescope/comm/Dome.in j- /usr/local/telescope/comm/Dome.out 150 "paddle command ccw"
domestop=/usr/local/telescope/comm/Dome.in stop /usr/local/telescope/comm/Dome.out 5 "stop complete"
domepark=/usr/local/telescope/comm/Dome.in Az:2.7 /usr/local/telescope/comm/Dome.out 150 "azimuth command complete" "error"
domeauto_on=/usr/local/telescope/comm/Dome.in auto /usr/local/telescope/comm/Dome.out
domeauto_off=/usr/local/telescope/comm/Dome.in off /usr/local/telescope/comm/Dome.out
domestatus=/usr/local/telescope/comm/Dome.in status /usr/local/telescope/comm/Dome.out 3 "Dome" "dome"

# Cover commands
coveropen=/usr/local/telescope/comm/Cover.in coveropen /usr/local/telescope/comm/Cover.out 60 "open complete"
coverclose=/usr/local/telescope/comm/Cover.in coverclose /usr/local/telescope/comm/Cover.out 60 "close complete"
coverstatus=/usr/local/telescope/comm/Cover.in status /usr/local/telescope/comm/Cover.out 3

